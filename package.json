{
  "version": "1.5.0",
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-departures",
  "title": "Departures",
  "description": "A raycast plugin to find travel information",
  "icon": "command-icon.png",
  "author": "rosvik",
  "owner": "variant-as",
  "categories": [
    "Web",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Departures",
      "subtitle": "View Public Transport Departures",
      "description": "Find departures from any stop place",
      "mode": "view",
      "arguments": []
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.2",
    "@raycast/utils": "^1.6.1",
    "cross-fetch": "^3.1.5",
    "graphql": "^16.6.0",
    "mapbox-gl": "^2.14.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
